    "express": "^4.17.3",
    "express-validator": "^6.2.0",
    "helmet": "^3.21.2",
    "nodemailer": "^6.3.0",
    "path": "^0.12.7",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.5",
