    "@types/nodemailer": "^6.2.1",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
